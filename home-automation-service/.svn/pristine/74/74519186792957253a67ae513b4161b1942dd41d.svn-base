CREATE  TABLE `weather` (

  `weather_id` INT NOT NULL AUTO_INCREMENT ,

  `station_id` VARCHAR(20) NULL ,

  `observation_time` TIMESTAMP NULL ,

  `local_time` TIMESTAMP NULL ,

  `weather` VARCHAR(100) NULL ,

  `temperature` INT NULL ,

  `relative_humidity` INT NULL ,

  `wind_string` VARCHAR(200) NULL ,

  `dewpoint` INT NULL ,

  `feelslike` INT NULL ,

  `visibility` DOUBLE NULL ,

  `precip_today` DOUBLE NULL ,

  PRIMARY KEY (`weather_id`) );